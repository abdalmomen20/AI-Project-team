import heapq

N = 5

def is_safe(state, row, col):
    for r, c in enumerate(state):
        if c == col or abs(c - col) == abs(r - row):
            return False
    return True

def heuristic(state):
    return N - len(state)

def astar_5_queens():
    pq = []
    heapq.heappush(pq, (heuristic([]), 0, []))

    while pq:
        f, g, state = heapq.heappop(pq)

        if len(state) == N:
            return state

        for col in range(N):
            if is_safe(state, len(state), col):
                new_state = state + [col]
                g_new = g + 1
                f_new = g_new + heuristic(new_state)
                heapq.heappush(pq, (f_new, g_new, new_state))

print(astar_5_queens())
